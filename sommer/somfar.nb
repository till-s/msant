Cell["We leave just the terms up to chi^2 and obtain", "Text"],

Cell[BoxData[
    \(2\ er\ l\ Sinh[chie\ kD]/chie\ \ chi/\((coe1 + chi^2\ coe2)\)\)], 
  "Input"],

Cell[CellGroupData[{

Cell[BoxData[
    \(?? kernlinks\)], "Input"],

Cell[BoxData[
    \("(1+er chi/nenn(-chi)) lam/chie^2"\)], "Print"],

Cell[BoxData[
    InterpretationBox[
      StyleBox[
        \(kernlinks[sommerfeld`Private`lam_]\  := \ 
          \((\((1\  + \ 
                    \((sommerfeld`Private`er*chi[sommerfeld`Private`lam])\)/
                      nenn[sommerfeld`Private`lam, \ \(-chi[#1]\)\ &\ ])\)*
                sommerfeld`Private`lam)\)/
            \((sommerfeld`Private`lam^2\  - \ sommerfeld`Private`er)\)\),
        ShowStringCharacters->True,
        NumberMarks->True],
      InputForm[ 
        Definition[ kernlinks]],
      Editable->True,
      AutoDelete->True]], "Print"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
    \(\(?wegint\)\)], "Input"],

Cell[BoxData[
    \("utility to compute complex line integrals \
wegint[kern,f,lam[t],t,ta,te,kr]:=Nintegrate[kern[lam] f[krho lam] \
Dt[lam,t],{t,ta,te}]"\)], "Print"]
}, Open  ]],

Cell[BoxData[
    \(kD := 0.1; \ er := 10; \ krho := 3\)], "Input"],

Cell[CellGroupData[{

