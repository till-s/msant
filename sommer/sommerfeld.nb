Cell[BoxData[
    \(SFI1[krho_, ker_] := 
      wegint[ker, Hankel02, \((1 + I)\) t, t, 
          If[krho > 0.1, \(-7\)/krho, \(-70\)], 0, krho] + \n
        wegint[ker, Hankel02, \((1 - E^\((\(-I\)\ fi)\))\), fi, 0, Pi, krho]
           + \nwegint[ker, Hankel02, 2 + \((1 - I)\) t, t, 0, 
          If[krho > 0.1, 7/krho, 70], krho]\)], "Input",
  InitializationCell->True],

Cell[BoxData[
    \(SFIgen[krho_, ker_, fun_, tend_] := 
      wegint[ker, fun, \((1 + I)\) t, t, \(-tend\), 0, krho] + \n
        wegint[ker, fun, \((1 - E^\((\(-I\)\ fi)\))\), fi, 0, Pi, krho] + \n
        wegint[ker, fun, 2 + \((1 - I)\) t, t, 0, tend, krho]\)], "Input",
  InitializationCell->True],

Cell["\<\
Eine  Integration vorwiegend auf der reellen Achse; lediglich zum \
Umgehen des Pols wird auf dem selben Kreisbogen wie oben ins Komplexe \
ausgewichen:\
\>", "Text"],

Cell[BoxData[
    \(SFIcirc[krho_, ker_] := 
      2\ \((wegint[ker, BesselJ[0, #]&, \((1 - E^\((\(-I\)\ fi)\))\), fi, 0, 
              Pi, krho] + 
            wegint[ker, BesselJ[0, #]&, 2 + t, t, 0, 
              If[krho > 0.1, 200/krho, 2000], krho])\)\)], "Input",
  InitializationCell->True]
